<h1>carts#checkout</h1>
<div class="col-md-12"> 
  <div class="group">
  </div>

  <table class="table table-hover">
    <thead> 
      <tr>
        <td> # </td>
        <td> ID </td>
        <td> Name </td>
        <td> Description </td>
      </tr>
    </thead>
    <tbody>
      <% @item.each do |i| %>
        <tr>
        <td> # </td>
        <td> <%= i.id %> </td>
        <td> <%= i.name %> </td>
        <td> <%= i.description %> </td>
        </tr>
      <% end %> 
    </tbody>
  </table>
  <%= link_to("Checkout", orders_path, :class => "btn btn-sm btn-default") %>
  <%= link_to("Back", carts_path, :class => "btn btn-sm btn-default") %>
</div>
