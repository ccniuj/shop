<div class="col-md-4 col-md-offset-4">
  <%= simple_form_for @mq do |f| %>
    <%= f.input :title, :input_html => { :class => "form-control"}, :label => "標題" %>
    <%= f.input :content, :input_html => { :class => "form-control"}, :label => "內容" %>
    <hr>
    <div class="form-actions">
      <%= f.submit "確認", :disable_with => 'Submiting...', :class => "btn btn-primary"%> 
      <%= link_to "返回", member_questions_path,  :class => "btn btn-default" %>
    </div>
  <% end %> 
</div>