<h2 class="text-center">客戶服務</h2>
<div class="col-md-12">
  <table class="table">
    <thead>
      <tr>
        <th>標題</th>
        <th>使用者</th>
        <th>狀態</th>
        <th>提問時間</th>
        <th colspan="2"></th>
      </tr>
    </thead>
    <tbody>
      <% @mqs.each do |mq| %> 
        <tr>
          <td> <%= link_to admin_member_question_path(mq.user) do %><%= mq.title %><% end %> </td>
          <td> <%= mq.user.id %> </td>
          <td> <%= mq.status%> </td>
          <td> <%= mq.created_at%> </td>
          <td> <% link_to("Edit", 
                           edit_admin_member_question_path(mq), 
                           :class => "btn btn-default btn-xs")%>

               <% link_to("Delete", 
                           admin_member_question_path(mq), 
                           :class => "btn btn-default btn-xs ",
                           :method => :delete,
                           data: { confirm: "Are you sure?" } )%></td>
        </tr>
      <% end %> 
    </tbody>
  </table>
</div>