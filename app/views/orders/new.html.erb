<h1>orders#new</h1>


<hr>
<%= simple_form_for @order do |f| %>
  <%= f.input :user_id, :input_html => { :class => "form-control"} %>
  <%= f.input :contact_id, :input_html => { :class => "form-control"} %>
  <%= f.input :pay_method, :input_html => { :class => "form-control"} %>
  <%= f.input :ship_method, :input_html => { :class => "form-control"} %>
  <%= f.input :status, :input_html => { :class => "form-control"} %>
  <%= f.input :total_price, :input_html => { :class => "form-control"} %>

<hr>
  <%= f.submit "Submit", :disable_with => 'Submitting...', :class => "btn btn-primary"%> 
<% end %> 
</div>
